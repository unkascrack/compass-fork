<?xml version="1.0"?>
<!DOCTYPE compass-core-mapping PUBLIC
        "-//Compass/Compass Core Mapping DTD 2.3//EN"
        "http://www.compass-project.org/dtd/compass-core-mapping-2.3.dtd">

<compass-core-mapping>

    <json-contract alias="contract1">
        <json-id name="id" />
    </json-contract>

    <json-contract alias="contract2">
        <json-property name="value" />
    </json-contract>

    <root-json-object alias="a" extends="contract1, contract2">
    </root-json-object>
    
</compass-core-mapping>
    
